{
    "logger": {
        "appenders": {
            "out": {
                "type": "console"
            },
            "task": {
                "type": "file",
                "filename": "logs/server.log",
                "maxLogSize": 1048576,
                "backups": 10
            }
        },
        "categories": {
            "default": {
                "appenders": [
                    "out",
                    "task"
                ],
                "level": "debug"
            },
            "task": {
                "appenders": [
                    "task"
                ],
                "level": "error"
            }
        }
    },
    "web": {
        "host": "mikrotik.com",
        "path": "/download",
        "changelog": "/download.rss",
        "changelog2": "/download/changelogs?ax=loadLog&val="
    },
    "releases": {
        "longterm": ">(?<release>[0-9a-zA-Z\\.]+)\\s\\(L",
        "stable": ">(?<release>[0-9a-zA-Z\\.]+)\\s\\(S",
        "testing": ">(?<release>[0-9a-zA-Z\\.]+)\\s\\(T",
        "development": ">(?<release>[0-9a-zA-Z\\.]+)\\s\\(D"
    },
    "cron": "*/2 * * * *",
    "downloadPath": "download",
    "redownload": false
}